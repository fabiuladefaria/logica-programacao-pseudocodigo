algoritmo "semnome"
var
    km, precototal: real
    dias: inteiro
inicio

      escreva("Qual a quantidade de km percorridos?")
      leia(km)
      escreva("Agora diga os dias utilizados:")
      leia(dias)
      precototal<- (km*0.20)+(90*dias)
      escreva("O preco total do alugal ficou: R$ " , precototal:0:2)

fimalgoritmo