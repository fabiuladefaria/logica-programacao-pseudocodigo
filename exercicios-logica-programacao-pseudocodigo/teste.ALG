algoritmo "semnome"
var
idade, anonasc, anoatual: inteiro
mes:caractere

inicio

escreva("Qual ano voce nasceu? ")
leia(anonasc)
escreva("Qual mes voce nasceu? ")
leia(mesnasc)
escreva("Qual mes estamos? ")
leia(mesatual)
escreva("Qual ano estamos? ")
leia(anoatual)

idade <- anoatual - anonasc)
caso 1


fimalgoritmo